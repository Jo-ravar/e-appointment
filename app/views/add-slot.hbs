                                    <div class="form-group">
                                        <label>start time</label><br>
                                        <select id="s1">
                                                            <script language="javascript" type="text/javascript"> 

                                                                for (var d = 0; d <= 24; d++) {
                                                                    document.write("<option>" + d + "</option>");

                                                                    function f1() {
                                                                        var a = document.getElementById("s1").value;
                                                                        var b = document.getElementById("s2").value;
                                                                        var c = a * 3600 + b * 60;
                                                                        document.getElementById('st').value = String(c);
                                                                    }

                                                                }
                                                            </script>
                                                            </select>
                                        <select id="s2">
                                                            <script language="javascript" type="text/javascript"> 

                                                                for (var d = 0; d <= 60; d++) {
                                                                    document.write("<option>" + d + "</option>");
                                                                }
                                                            </script>
                                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label>end time</label><br>
                                        <select id="s3">
                                                            <script language="javascript" type="text/javascript"> 

                                                                for (var d = 0; d <= 24; d++) {
                                                                    document.write("<option>" + d + "</option>");

                                                                    function f2() {
                                                                        var a = document.getElementById("s3").value;
                                                                        var b = document.getElementById("s4").value;
                                                                        var c = a * 3600 + b * 60;
                                                                        document.getElementById('et').value = String(c);
                                                                    }

                                                                }
                                                            </script>
                                                            </select>
                                        <select id="s4">
                                                            <script language="javascript" type="text/javascript"> 

                                                                for (var d = 0; d <= 60; d++) {
                                                                    document.write("<option>" + d + "</option>");
                                                                }
                                                            </script>
                                                        </select>
                                    </div>
